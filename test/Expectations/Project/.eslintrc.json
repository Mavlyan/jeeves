{
  "env": {
    "node": true,
    "browser": true,
    "jquery": true,
    "amd": true,
    "prototypejs": true
  },
  "globals": {},
  "parserOptions": {
    "ecmaVersion": 6
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      "error",
      2
    ],
    "keyword-spacing": [
      "error",
      {
        "before": true,
        "after": true
      }
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "no-multiple-empty-lines": "error",
    "no-unused-vars": "warn",
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "space-before-blocks": [
      "error",
      "always"
    ]
  }
}
